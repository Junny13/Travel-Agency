<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.happytraveler">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HappyTraveler"
        tools:targetApi="31"
        >

        <activity
            android:name=".UpdateDeletePakEk"
            android:exported="false" />
        <activity
            android:name=".UpdateDeleteEkdromi"
            android:exported="false" />
        <activity
            android:name=".UpdateDeleteTaksid"
            android:exported="false" />
        <activity
            android:name=".ButtonPressed"
            android:exported="false" />
        <activity
            android:name=".ButtonPressedPinakas"
            android:exported="false" />
        <activity
            android:name=".ButtonPressedPakEkdr"
            android:exported="false"
            android:parentActivityName=".MainDrawerActivity" />
        <activity
            android:name=".ButtonPressedEkdr"
            android:exported="false"
            android:parentActivityName=".MainDrawerActivity" />
        <activity
            android:name=".MainDrawerActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.HappyTraveler.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>